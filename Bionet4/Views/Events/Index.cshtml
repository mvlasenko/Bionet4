@using Bionet4.ViewModels
@model EventsViewModel
@{
    ViewBag.Title = Model.Intro.Name;
    ViewBag.Description = Model.Intro.Description;
}

<section id="main">
    <!-- header -->
    @Html.Partial("_HeaderPartial")
    <!-- / header -->
    <!-- Main Content -->
    @Html.Partial("_IntroPartial", Model.Intro)
    @Html.Partial("_EventsPartial", Model.Events)
    <!-- /Main Content -->
</section>